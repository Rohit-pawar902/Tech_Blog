<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServ</servlet-name>
        <servlet-class>com.tech.servlet.LoginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServ</servlet-name>
        <servlet-class>com.tech.servlet.LogoutServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditServ</servlet-name>
        <servlet-class>com.tech.servlet.EditServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostSubmitServ</servlet-name>
        <servlet-class>PostSubmitServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServ</servlet-name>
        <url-pattern>/LoginServ</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServ</servlet-name>
        <servlet-class>com.tech.servlet.RegisterServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServ</servlet-name>
        <url-pattern>/RegisterServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServ</servlet-name>
        <url-pattern>/LogoutServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditServ</servlet-name>
        <url-pattern>/EditServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostSubmitServ</servlet-name>
        <url-pattern>/PostSubmitServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
